<?xml version='1.0' encoding='utf-8'?>
<widget id="com.koine.bolts" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Bolts</name>
    <description>Bolts - deu fome? Pede no Bolts.</description>
    <author email="hi@ionicframework" href="http://ionicframework.com/">Koine Development</author>
    <content src="index.html" />
    <access origin="*" />
    <allow-navigation href="http://ionic.local/*" />
    <allow-navigation href="http://172.24.3.169:8100" />
    <allow-navigation href="http://192.168.43.184:8100" />
    <allow-navigation href="http://10.0.0.104:8100" />
    <allow-navigation href="http://172.24.1.222:8100" />
    <allow-navigation href="http://172.24.5.136:8100" />
